{
  "manifest_version": 2,
  "name": "Twitter Fixer",
  "description": "Fixes twitter scroll",
  "version": "1.0",
  "browser_action": {
    "default_icon": "tw.png"
  },
  "content_scripts":
    [
      {
        "matches": ["*://*/*"],
         "js": ["tw.js"],
         "run_at": "document_end"
      }
    ]
}
